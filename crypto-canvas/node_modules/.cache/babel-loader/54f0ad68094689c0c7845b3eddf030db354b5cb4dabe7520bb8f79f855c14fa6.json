{"ast":null,"code":"import Axios from 'axios';\nimport CorrelationMatrix from './components/CorrelationMatrix.vue';\nimport VolatilityComparison from './components/VolatilityComparison.vue';\nimport BitcoinHalving from './components/BitcoinHalving.vue';\nimport PriceChart from './components/PriceOverTime.vue';\nimport MarketCapTreeMap from './components/MarketCapTreeMap.vue';\nimport CoinStats from './components/CoinStats.vue'; // Import the CoinStats component\n\nexport default {\n  name: 'App',\n  components: {\n    CorrelationMatrix,\n    VolatilityComparison,\n    BitcoinHalving,\n    PriceChart,\n    MarketCapTreeMap,\n    CoinStats\n  },\n  data() {\n    return {\n      currentTab: 'MarketOverview',\n      selectedTimeRange: 'Last 6 Months',\n      selectedCoins: ['Bitcoin', 'Ethereum', 'Dogecoin'],\n      availableCoins: []\n    };\n  },\n  created() {\n    this.fetchAvailableCoins();\n  },\n  methods: {\n    fetchAvailableCoins() {\n      Axios.get('http://127.0.0.1:5000/available-coins') // Update with your API URL if different\n      .then(response => {\n        this.availableCoins = response.data.coins;\n      }).catch(error => {\n        console.error('Error fetching available coins:', error);\n      });\n    }\n  },\n  watch: {\n    selectedTimeRange(newVal) {\n      console.log('Selected Time Range:', newVal);\n      // Additional logic to handle the change in time range\n    },\n\n    selectedCoins(newVal) {\n      console.log('Selected Coins:', newVal);\n      // Additional logic to handle the change in time range\n    }\n  }\n};","map":{"version":3,"names":["Axios","CorrelationMatrix","VolatilityComparison","BitcoinHalving","PriceChart","MarketCapTreeMap","CoinStats","name","components","data","currentTab","selectedTimeRange","selectedCoins","availableCoins","created","fetchAvailableCoins","methods","get","then","response","coins","catch","error","console","watch","newVal","log"],"sources":["C:\\Projects\\CryptoCanvas\\crypto-canvas\\src\\App.vue"],"sourcesContent":["<template>\r\n  <v-app>\r\n    <!-- Navigation Drawer -->\r\n    <v-navigation-drawer app class=\"custom-drawer\">\r\n      <v-list dense>\r\n        <v-list-item link @click=\"currentTab = 'MarketOverview'\" class=\"white--text\">\r\n          <v-list-item-icon>\r\n            <v-icon>mdi-view-dashboard</v-icon>\r\n          </v-list-item-icon>\r\n          <v-list-item-content>\r\n            <v-list-item-title>Market Overview</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item>\r\n        <v-list-item link @click=\"currentTab = 'VolatilityComparison'\" class=\"white--text\">\r\n          <v-list-item-icon>\r\n            <v-icon>mdi-chart-line</v-icon>\r\n          </v-list-item-icon>\r\n          <v-list-item-content>\r\n            <v-list-item-title>Volatility Comparison</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item>\r\n      </v-list>\r\n    </v-navigation-drawer>\r\n\r\n    <v-app-bar app>\r\n      <img\r\n        src=\"@/assets/crypto-canvas-high-resolution-logo-transparent.png\"\r\n        class=\"my-logo\"\r\n        alt=\"Logo\"\r\n        height=\"40\"\r\n      />\r\n      <v-select\r\n        v-model=\"selectedTimeRange\"\r\n        :items=\"['Last 6 Months', '1 Year', '2 Years', '4 Years', 'BTC Halving 2016-07-09', 'BTC Halving 2020-05-11', ]\"\r\n        label=\"Select Time Range\"\r\n        style=\"max-width: 180px;\"\r\n      ></v-select>\r\n      <v-select\r\n        v-model=\"selectedCoins\"\r\n        :items=\"availableCoins\"\r\n        label=\"Select Coins\"\r\n        multiple\r\n        dense\r\n        style=\"max-width: 1000px;\"\r\n      ></v-select>\r\n    </v-app-bar>\r\n\r\n    <!-- Main Content -->\r\n    <v-main>\r\n      <div v-if=\"currentTab === 'MarketOverview'\" class=\"market-overview-container\">\r\n        <div class=\"charts-container\">\r\n          <PriceChart :selectedTimeRange=\"selectedTimeRange\" :selectedCoins=\"selectedCoins\" />\r\n         <MarketCapTreeMap :selectedTimeRange=\"selectedTimeRange\" :selectedCoins=\"selectedCoins\" />\r\n        </div>\r\n        <CoinStats :selectedTimeRange=\"selectedTimeRange\" :selectedCoins=\"selectedCoins\" class=\"coin-stats-container\" />\r\n      </div>\r\n      <div v-if=\"currentTab === 'VolatilityComparison'\">\r\n          <correlation-matrix :selected-time-range=\"selectedTimeRange\" :selected-coins=\"selectedCoins\"></correlation-matrix>\r\n          <VolatilityComparison :selected-time-range=\"selectedTimeRange\" :selected-coins=\"selectedCoins\"></VolatilityComparison>\r\n          <BitcoinHalving />\r\n      </div>\r\n    </v-main>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport Axios from 'axios'\r\nimport CorrelationMatrix from './components/CorrelationMatrix.vue'\r\nimport VolatilityComparison from './components/VolatilityComparison.vue'\r\nimport BitcoinHalving from './components/BitcoinHalving.vue'\r\nimport PriceChart from './components/PriceOverTime.vue'\r\nimport MarketCapTreeMap from './components/MarketCapTreeMap.vue'\r\nimport CoinStats from './components/CoinStats.vue' // Import the CoinStats component\r\n\r\nexport default {\r\n  name: 'App',\r\n  components: {\r\n    CorrelationMatrix,\r\n    VolatilityComparison,\r\n    BitcoinHalving,\r\n    PriceChart,\r\n    MarketCapTreeMap,\r\n    CoinStats\r\n\r\n  },\r\n  data () {\r\n    return {\r\n      currentTab: 'MarketOverview',\r\n      selectedTimeRange: 'Last 6 Months',\r\n      selectedCoins: ['Bitcoin', 'Ethereum', 'Dogecoin'],\r\n      availableCoins: []\r\n    }\r\n  },\r\n  created () {\r\n    this.fetchAvailableCoins()\r\n  },\r\n  methods: {\r\n    fetchAvailableCoins () {\r\n      Axios.get('http://127.0.0.1:5000/available-coins') // Update with your API URL if different\r\n        .then(response => {\r\n          this.availableCoins = response.data.coins\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching available coins:', error)\r\n        })\r\n    }\r\n  },\r\n  watch: {\r\n    selectedTimeRange (newVal) {\r\n      console.log('Selected Time Range:', newVal)\r\n      // Additional logic to handle the change in time range\r\n    },\r\n    selectedCoins (newVal) {\r\n      console.log('Selected Coins:', newVal)\r\n      // Additional logic to handle the change in time range\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.v-select {\r\n  border: none; /* Remove border for a cleaner look */\r\n  border-radius: 25px; /* Fully rounded corners for a pill-like shape */\r\n  padding: 5px 20px; /* Adjust padding for aesthetic spacing */\r\n  background-color: #273c75; /* Slightly darker background for a subtle depth */\r\n  box-shadow: 0 2px 12px 0 rgba(0,0,0,0.2); /* Soft shadow for a floating effect */\r\n}\r\n\r\n/* Style for the dropdown text, making it bold and larger */\r\n.v-select .v-select__selection, .v-select__slot {\r\n  color: #ffffff; /* White text for better visibility */\r\n  font-weight: bold; /* Bold font for emphasis */\r\n  font-size: 1rem; /* Larger font size for readability */\r\n}\r\n\r\n/* Style for the dropdown icon */\r\n.v-select .v-icon {\r\n  color: #ffffff; /* White icon to match the text */\r\n}\r\n\r\n/* Style for the dropdown list */\r\n.v-menu__content {\r\n  border-radius: 15px; /* Rounded corners for the dropdown list */\r\n  box-shadow: 0 5px 15px 0 rgba(0,0,0,0.2); /* More pronounced shadow for the dropdown list */\r\n}\r\n\r\n/* Style for the dropdown items */\r\n.v-list-item {\r\n  padding: 10px 20px; /* More padding for a touch-friendly design */\r\n}\r\n\r\n/* Hover state for dropdown items */\r\n.v-list-item:hover {\r\n  background-color: #7d2ae8; /* A different shade of purple for the hover state */\r\n  color: white; /* White text for the hover state */\r\n  border-radius: 15px; /* Rounded corners for the hover state */\r\n}\r\n.v-app-bar {\r\n  background: linear-gradient(90deg, rgba(39,37,89,1) 0%, rgba(69,31,152,1) 10%, rgba(0,212,255,1) 100%) !important;\r\n}\r\n\r\n.v-btn, .v-btn-text, .v-icon {\r\n  color: #7d2ae8 !important; /* This sets the accent color for text and icons in the app bar */\r\n}\r\n\r\n.my-logo {\r\n  margin-right: 10px;\r\n  margin-left: 10px; /* Add some spacing between logo and title */\r\n}\r\n\r\n.custom-drawer {\r\n  background-color: #272559; /* Background color for the drawer */\r\n}\r\n\r\n.custom-drawer .v-list-item {\r\n  color: #7d2ae8; /* Accent color for list items */\r\n}\r\n\r\n.custom-drawer .v-list-item--active {\r\n  background-color: rgba(125, 42, 232, 0.2); /* Lighter accent for active items */\r\n}\r\n\r\n.custom-drawer .v-icon {\r\n  color: #7d2ae8;\r\n}\r\n\r\n.v-application .custom-drawer .v-list-item,\r\n.v-application .custom-drawer .v-icon {\r\n  color: white; /* Changed to white for better visibility */\r\n}\r\n\r\n/* Navigation Drawer active list item */\r\n.v-application .custom-drawer .v-list-item.v-list-item--active {\r\n  background-color: rgba(125, 42, 232, 0.2);\r\n}\r\n\r\nbody, .v-application {\r\n  background-color: #ffffff; /* Light purple color */\r\n}\r\n.market-overview-container {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: flex-start;\r\n}\r\n\r\n.charts-container {\r\n  flex-grow: 1; /* Allow the charts container to take up the available space */\r\n}\r\n\r\n/* Ensure that CoinStats takes only the necessary space */\r\n.coin-stats-container {\r\n  width: 50%; /* Or whatever width you prefer */\r\n  margin-top: 0; /* Align to the top */\r\n}\r\n\r\n</style>\r\n"],"mappings":"AAkEA,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,iBAAgB,MAAO,oCAAmC;AACjE,OAAOC,oBAAmB,MAAO,uCAAsC;AACvE,OAAOC,cAAa,MAAO,iCAAgC;AAC3D,OAAOC,UAAS,MAAO,gCAA+B;AACtD,OAAOC,gBAAe,MAAO,mCAAkC;AAC/D,OAAOC,SAAQ,MAAO,4BAA2B,EAAE;;AAEnD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVP,iBAAiB;IACjBC,oBAAoB;IACpBC,cAAc;IACdC,UAAU;IACVC,gBAAgB;IAChBC;EAEF,CAAC;EACDG,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,UAAU,EAAE,gBAAgB;MAC5BC,iBAAiB,EAAE,eAAe;MAClCC,aAAa,EAAE,C