ist-item-icon>\n            <v-icon>mdi-view-dashboard</v-icon>\n          </v-list-item-icon>\n          <v-list-item-content>\n            <v-list-item-title>Market Overview</v-list-item-title>\n          </v-list-item-content>\n          <v-list-item-content>\n            <v-list-item-title>Price Chart</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n    </v-navigation-drawer>\n\n    <v-app-bar app>\n      <v-toolbar-title>Crypto Canvas Dashboard</v-toolbar-title>\n      <v-select\n        v-model=\"selectedTimeRange\"\n        :items=\"['Last 6 Months', 'Last 12 Months', 'Last 24 Months', 'Last 48 Months']\"\n        label=\"Select Time Range\"\n        style=\"max-width: 180px;\"\n      ></v-select>\n      <v-select\n        v-model=\"selectedCoins\"\n        :items=\"availableCoins\"\n        label=\"Select Coins\"\n        multiple\n        dense\n        style=\"max-width: 500px;\"\n      ></v-select>\n    </v-app-bar>\n\n    <!-- Main Content -->\n    <v-main>\n      <div v-if=\"currentTab === 'VolatilityComparison'\">\n        <CorrelationMatrix />\n        <VolatilityComparison />\n        <BitcoinHalving />\n      </div>\n      <div v-if=\"currentTab === 'MarketOverview'\">\n        <PriceChart :selectedTimeRange=\"selectedTimeRange\" :selectedCoins=\"selectedCoins\" />\n        <MarketCapTreeMap :selectedTimeRange=\"selectedTimeRange\" :selectedCoins=\"selectedCoins\" />\n      </div>\n    </v-main>\n  </v-app>\n</template>\n\n<script>\nimport CorrelationMatrix from './components/CorrelationMatrix.vue'\nimport VolatilityComparison from './components/VolatilityComparison.vue'\nimport BitcoinHalving from './components/BitcoinHalving.vue'\nimport PriceChart from './components/PriceOverTime.vue'\nimport MarketCapTreeMap from './components/MarketCapTreeMap.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    CorrelationMatrix,\n    VolatilityComparison,\n    BitcoinHalving,\n    PriceChart,\n    MarketCapTreeMap\n  },\n  data () {\n    return {\n      currentTab: 'VolatilityComparison',\n      selectedTimeRange: 'Last 6 Months',\n      selectedCoins: ['Bitcoin', 'Ethereum', 'XRP'],\n      availableCoins: ['Bitcoin', 'Ethereum', 'XRP', 'Litecoin', 'Cardano']\n    }\n  },\n  watch: {\n    selectedTimeRange (newVal) {\n      console.log('Selected Time Range:', newVal)\n      // Additional logic to handle the change in time range\n    },\n    selectedCoins (newVal) {\n      console.log('Selected Coins:', newVal)\n      // Additional logic to handle the change in time range\n    }\n  }\n}\n</script>\n\n<style>\n/* Add any additional styles if necessary */\n</style>\n"],"mappings":"AA6DA,OAAOA,iBAAgB,MAAO,oCAAmC;AACjE,OAAOC,oBAAmB,MAAO,uCAAsC;AACvE,OAAOC,cAAa,MAAO,iCAAgC;AAC3D,OAAOC,UAAS,MAAO,gCAA+B;AACtD,OAAOC,gBAAe,MAAO,mCAAkC;AAE/D,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVN,iBAAiB;IACjBC,oBAAoB;IACpBC,cAAc;IACdC,UAAU;IACVC;EACF,CAAC;EACDG,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,UAAU,EAAE,sBAAsB;MAClCC,iBAAiB,EAAE,eAAe;MAClCC,aAAa,EAAE,CAAC,SAAS,EAAE,UAAU,EAAE,KAAK,CAAC;MAC7CC,cAAc,EAAE,CAAC,SAAS,EAAE,UAAU,EAAE,KAAK,EAAE,UAAU,EAAE,SAAS;IACtE;EACF,CAAC;EACDC,KAAK,EAAE;IACLH,iBAAgBA,CAAGI,MAAM,EAAE;MACzBC,OAAO,CAACC,GAAG,CAAC,sBAAsB,EAAEF,MAAM;MAC1C;IACF,CAAC;;IACDH,aAAYA,CAAGG,MAAM,EAAE;MACrBC,OAAO,CAACC,GAAG,CAAC,iBAAiB,EAAEF,MAAM;MACrC;IACF;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}