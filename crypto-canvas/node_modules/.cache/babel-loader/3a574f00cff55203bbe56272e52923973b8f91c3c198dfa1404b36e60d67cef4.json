{"ast":null,"code":"import CorrelationMatrix from './components/CorrelationMatrix.vue';\nimport VolatilityComparison from './components/VolatilityComparison.vue';\nimport BitcoinHalving from './components/BitcoinHalving.vue';\nimport PriceChart from './components/PriceOverTime.vue';\nexport default {\n  name: 'App',\n  components: {\n    CorrelationMatrix,\n    VolatilityComparison,\n    BitcoinHalving,\n    PriceChart\n  },\n  data() {\n    return {\n      currentTab: 'VolatilityComparison',\n      selectedTimeRange: 'Last 6 Months'\n    };\n  },\n  watch: {\n    selectedTimeRange(newVal) {\n      console.log('Selected Time Range:', newVal);\n      // Additional logic to handle the change in time range\n    }\n  }\n};","map":{"version":3,"names":["CorrelationMatrix","VolatilityComparison","BitcoinHalving","PriceChart","name","components","data","currentTab","selectedTimeRange","watch","newVal","console","log"],"sources":["/Users/stefanstapelberg/Desktop/FSD_PROJECT/CryptoCanvas/crypto-canvas/src/App.vue"],"sourcesContent":["<template>\n  <v-app>\n    <!-- Navigation Drawer -->\n    <v-navigation-drawer app>\n      <v-list dense>\n        <v-list-item link @click=\"currentTab = 'VolatilityComparison'\">\n          <v-list-item-icon>\n            <v-icon>mdi-chart-line</v-icon>\n          </v-list-item-icon>\n          <v-list-item-content>\n            <v-list-item-title>Volatility Comparison</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n        <v-list-item link @click=\"currentTab = 'MarketOverview'\">\n          <v-list-item-icon>\n            <v-icon>mdi-view-dashboard</v-icon>\n          </v-list-item-icon>\n          <v-list-item-content>\n            <v-list-item-title>Market Overview</v-list-item-title>\n          </v-list-item-content>\n          <v-list-item-content>\n            <v-list-item-title>Price Chart</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n    </v-navigation-drawer>\n\n    <v-app-bar app>\n      <v-toolbar-title>Crypto Canvas Dashboard</v-toolbar-title>\n      <v-spacer></v-spacer>\n      <v-select\n        v-model=\"selectedTimeRange\"\n        :items=\"['Last 6 Months', 'Last 12 Months', 'Last 24 Months', 'Last 48 Months']\"\n        label=\"Select Time Range\"\n      ></v-select>\n    </v-app-bar>\n\n    <!-- Main Content -->\n    <v-main>\n      <div v-if=\"currentTab === 'VolatilityComparison'\">\n        <CorrelationMatrix />\n        <VolatilityComparison />\n        <BitcoinHalving />\n      </div>\n      <div v-if=\"currentTab === 'MarketOverview'\">\n        <PriceChart :selectedTimeRange=\"selectedTimeRange\" />\n      </div>\n    </v-main>\n  </v-app>\n</template>\n\n<script>\nimport CorrelationMatrix from './components/CorrelationMatrix.vue'\nimport VolatilityComparison from './components/VolatilityComparison.vue'\nimport BitcoinHalving from './components/BitcoinHalving.vue'\nimport PriceChart from './components/PriceOverTime.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    CorrelationMatrix,\n    VolatilityComparison,\n    BitcoinHalving,\n    PriceChart\n  },\n  data () {\n    return {\n      currentTab: 'VolatilityComparison',\n      selectedTimeRange: 'Last 6 Months'\n    }\n  },\n  watch: {\n    selectedTimeRange (newVal) {\n      console.log('Selected Time Range:', newVal)\n      // Additional logic to handle the change in time range\n    }\n  }\n}\n</script>\n\n<style>\n/* Add any additional styles if necessary */\n</style>\n"],"mappings":"AAoDA,OAAOA,iBAAgB,MAAO,oCAAmC;AACjE,OAAOC,oBAAmB,MAAO,uCAAsC;AACvE,OAAOC,cAAa,MAAO,iCAAgC;AAC3D,OAAOC,UAAS,MAAO,gCAA+B;AAEtD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVL,iBAAiB;IACjBC,oBAAoB;IACpBC,cAAc;IACdC;EACF,CAAC;EACDG,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,UAAU,EAAE,sBAAsB;MAClCC,iBAAiB,EAAE;IACrB;EACF,CAAC;EACDC,KAAK,EAAE;IACLD,iBAAgBA,CAAGE,MAAM,EAAE;MACzBC,OAAO,CAACC,GAAG,CAAC,sBAAsB,EAAEF,MAAM;MAC1C;IACF;EACF;AACF"},"metadata":{},"sourceType":