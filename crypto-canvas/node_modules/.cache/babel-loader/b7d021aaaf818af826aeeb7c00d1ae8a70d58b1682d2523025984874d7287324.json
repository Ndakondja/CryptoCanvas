{"ast":null,"code":"import Axios from 'axios';\nimport CorrelationMatrix from './components/CorrelationMatrix.vue';\nimport VolatilityComparison from './components/VolatilityComparison.vue';\nimport BitcoinHalving from './components/BitcoinHalving.vue';\nimport PriceChart from './components/PriceOverTime.vue';\nimport MarketCapTreeMap from './components/MarketCapTreeMap.vue';\nexport default {\n  name: 'App',\n  components: {\n    CorrelationMatrix,\n    VolatilityComparison,\n    BitcoinHalving,\n    PriceChart,\n    MarketCapTreeMap\n  },\n  data() {\n    return {\n      currentTab: 'VolatilityComparison',\n      selectedTimeRange: 'Last 6 Months',\n      selectedCoins: ['Bitcoin', 'Ethereum', 'Dogecoin', 'Litecoin'],\n      availableCoins: []\n    };\n  },\n  created() {\n    this.fetchAvailableCoins();\n  },\n  methods: {\n    fetchAvailableCoins() {\n      Axios.get('http://127.0.0.1:5000/available-coins') // Update with your API URL if different\n      .then(response => {\n        this.availableCoins = response.data.coins;\n      }).catch(error => {\n        console.error('Error fetching available coins:', error);\n      });\n    }\n  },\n  watch: {\n    selectedTimeRange(newVal) {\n      console.log('Selected Time Range:', newVal);\n      // Additional logic to handle the change in time range\n    },\n\n    selectedCoins(newVal) {\n      console.log('Selected Coins:', newVal);\n      // Additional logic to handle the change in time range\n    }\n  }\n};","map":{"version":3,"names":["Axios","CorrelationMatrix","VolatilityComparison","BitcoinHalving","PriceChart","MarketCapTreeMap","name","components","data","currentTab","selectedTimeRange","selectedCoins","availableCoins","created","fetchAvailableCoins","methods","get","then","response","coins","catch","error","console","watch","newVal","log"],"sources":["/Users/stefanstapelberg/Desktop/FSD_PROJECT/CryptoCanvas/crypto-canvas/src/App.vue"],"sourcesContent":["<template>\n  <v-app>\n    <!-- Navigation Drawer -->\n    <v-navigation-drawer app class=\"custom-drawer\">\n      <v-list dense>\n        <v-list-item link @click=\"currentTab = 'MarketOverview'\" class=\"white--text\">\n          <v-list-item-icon>\n            <v-icon>mdi-view-dashboard</v-icon>\n          </v-list-item-icon>\n          <v-list-item-content>\n            <v-list-item-title>Market Overview</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n        <v-list-item link @click=\"currentTab = 'VolatilityComparison'\" class=\"white--text\">\n          <v-list-item-icon>\n            <v-icon>mdi-chart-line</v-icon>\n          </v-list-item-icon>\n          <v-list-item-content>\n            <v-list-item-title>Volatility Comparison</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n    </v-navigation-drawer>\n\n    <v-app-bar app>\n      <img\n        src=\"@/assets/crypto-canvas-high-resolution-logo-transparent.png\"\n        class=\"my-logo\"\n        alt=\"Logo\"\n        height=\"40\"\n      />\n      <v-select\n        v-model=\"selectedTimeRange\"\n        :items=\"['Last 6 Months', 'Last 12 Months', 'Last 24 Months', 'Last 48 Months', 'BTC Halving 2016-07-09', 'BTC Halving 2020-05-11', ]\"\n        label=\"Select Time Range\"\n        style=\"max-width: 180px;\"\n      ></v-select>\n      <v-select\n        v-model=\"selectedCoins\"\n        :items=\"availableCoins\"\n        label=\"Select Coins\"\n        multiple\n        dense\n        style=\"max-width: 500px;\"\n      ></v-select>\n    </v-app-bar>\n\n    <!-- Main Content -->\n    <v-main>\n      <div v-if=\"currentTab === 'MarketOverview'\">\n        <PriceChart :selectedTimeRange=\"selectedTimeRange\" :selectedCoins=\"selectedCoins\" />\n        <MarketCapTreeMap :selectedTimeRange=\"selectedTimeRange\" :selectedCoins=\"selectedCoins\" />\n      </div>\n      <div v-if=\"currentTab === 'VolatilityComparison'\">\n      <correlation-matrix\n        :selected-time-range=\"selectedTimeRange\"\n        :selected-coins=\"selectedCoins\">\n      </correlation-matrix>\n        <VolatilityComparison />\n        <BitcoinHalving />\n      </div>\n    </v-main>\n  </v-app>\n</template>\n\n<script>\nimport Axios from 'axios'\nimport CorrelationMatrix from './components/CorrelationMatrix.vue'\nimport VolatilityComparison from './components/VolatilityComparison.vue'\nimport BitcoinHalving from './components/BitcoinHalving.vue'\nimport PriceChart from './components/PriceOverTime.vue'\nimport MarketCapTreeMap from './components/MarketCapTreeMap.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    CorrelationMatrix,\n    VolatilityComparison,\n    BitcoinHalving,\n    PriceChart,\n    MarketCapTreeMap\n  },\n  data () {\n    return {\n      currentTab: 'VolatilityComparison',\n      selectedTimeRange: 'Last 6 Months',\n      selectedCoins: ['Bitcoin', 'Ethereum', 'Dogecoin', 'Litecoin'],\n      availableCoins: []\n    }\n  },\n  created () {\n    this.fetchAvailableCoins()\n  },\n  methods: {\n    fetchAvailableCoins () {\n      Axios.get('http://127.0.0.1:5000/available-coins') // Update with your API URL if different\n        .then(response => {\n          this.availableCoins = response.data.coins\n        })\n        .catch(error => {\n          console.error('Error fetching available coins:', error)\n        })\n    }\n  },\n  watch: {\n    selectedTimeRange (newVal) {\n      console.log('Selected Time Range:', newVal)\n      // Additional logic to handle the change in time range\n    },\n    selectedCoins (newVal) {\n      console.log('Selected Coins:', newVal)\n      // Additional logic to handle the change in time range\n    }\n  }\n}\n</script>\n\n<style>\n/* Global styles */\n\n/* App Bar */\n.v-application .v-app-bar {\n  background: linear-gradient(90deg, rgba(39,37,89,1) 0%, rgba(69,31,152,1) 10%, rgba(0,212,255,1) 100%);\n}\n\n/* App Bar text and icons */\n.v-application .v-app-bar .v-btn,\n.v-application .v-app-bar .v-btn-text,\n.v-application .v-app-bar .v-icon {\n  color: #7d2ae8;\n}\n\n/* Navigation Drawer */\n.v-application .v-navigation-drawer.custom-drawer {\n  background-color: #272559;\n}\n\n/* Navigation Drawer list item text and icons */\n.v-application .custom-drawer .v-list-item,\n.v-application .custom-drawer .v-icon {\n  color: white; /* Changed to white for better visibility */\n}\n\n/* Navigation Drawer active list item */\n.v-application .custom-drawer .v-list-item.v-list-item--active {\n  background-color: rgba(125, 42, 232, 0.2);\n}\n</style>\n"],"mappings":"AAkEA,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,iBAAgB,MAAO,oCAAmC;AACjE,OAAOC,oBAAmB,MAAO,uCAAsC;AACvE,OAAOC,cAAa,MAAO,iCAAgC;AAC3D,OAAOC,UAAS,MAAO,gCAA+B;AACtD,OAAOC,gBAAe,MAAO,mCAAkC;AAE/D,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVN,iBAAiB;IACjBC,oBAAoB;IACpBC,cAAc;IACdC,UAAU;IACVC;EACF,CAAC;EACDG,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,UAAU,EAAE,sBAAsB;MAClCC,iBAAiB,EAAE,eAAe;MAClCC,aAAa,EAAE,CAAC,SAAS,EAAE,UAAU,EAAE,UAAU,EAAE,UAAU,CAAC;MAC9DC,cAAc,EAAE;IAClB;EACF,CAAC;EACDC,OAAMA,CAAA,EAAK;IACT,IAAI,CAACC,mBAAmB,CAAC;EAC3B,CAAC;EACDC,OAAO,EAAE;IACPD,mBAAkBA,CAAA,EAAK;MACrBd,KAAK,CAACgB,GAAG,CAAC,uCAAuC,EAAE;MAAA,CAChDC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACN,cAAa,GAAIM,QAAQ,CAACV,IAAI,CAACW,KAAI;MAC1C,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,iCAAiC,EAAEA,KAAK;MACxD,CAAC;IACL;EACF,CAAC;EACDE,KAAK,EAAE;IACLb,iBAAgBA,CAAGc,MAAM,EAAE;MACzBF,OAAO,CAACG,GAAG,CAAC,sBAAsB,EAAED,MAAM;MAC1C;IACF,CAAC;;IACDb,aAAYA,CAAGa,MAAM,EAAE;MACrBF,OAAO,CAACG,GAAG,CAAC,iBAAiB,EAAED,MAAM;MACrC;IACF;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}